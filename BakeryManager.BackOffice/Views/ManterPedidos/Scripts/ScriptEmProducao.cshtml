<script type="text/javascript">


    function VisualizarProducao(IdProduto) {
        window.location.href = "@Url.Action("ProduzirPedido","ProducaoPorPedido")/" + IdProduto;
    }

    $(function () {

        $.ajax({

            url: '@Url.Action("GetListaPedidosEmProducao", "ManterPedidos")',
            success: function (data) {

                $("#emproducao").html(data);
                $('[data-toggle="tooltip"]').tooltip();


            }

        });

    });
</script>

