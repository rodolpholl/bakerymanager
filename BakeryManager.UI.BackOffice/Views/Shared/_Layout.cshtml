<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Bakery Manager - @DateTime.Today.Year.ToString("YYYY")">
    <meta name="author" content="Angelorum Sistemas">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Bakery Manager</title>

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/metisMenu.min.css" rel="stylesheet" />
    <link href="~/Content/sb-admin-3.css" rel="stylesheet" />
    <link href="~/Content/font-awesome.css" rel="stylesheet" type="text/css" />
    
    <link href="~/Content/kendo.common.min.css" rel="stylesheet" />
    <link href="~/Content/kendo.bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/kendo.bootstrap.mobile.min.css" rel="stylesheet" />
    <link href="~/Content/UploadfyContent/uploadify.css" rel="stylesheet" />

    <link href="~/Content/SiteCustom.css" rel="stylesheet" />
    <link href="~/Content/rateit/rateit.css" rel="stylesheet" />
    

    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

    
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/jquery-2.1.4.intellisense.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/Scripts/jquery.easing.1.3.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.bootstrap.min.js"></script>
    <script src="~/Scripts/kendo.all.min.js"></script>
    <script src="~/Scripts/kendo.aspnetmvc.min.js"></script>
    <script src="~/Scripts/kendo.culture.pt-BR.min.js"></script>
    <script src="~/Scripts/kendo.messages.pt-BR.min.js"></script>
    <script src="~/Scripts/notifyui.js"></script>
    <script src="~/Scripts/rateit/jquery.rateit.min.js"></script>
    <script src="~/Content/UploadfyContent/jquery.uploadify.min.js"></script>
    <script src="~/Scripts/utils.js"></script>
    <script src="~/Scripts/metisMenu.min.js"></script>
    <script src="~/Scripts/sb-admin-2.js"></script>
    <script src="~/Scripts/numeral.min.js"></script>

    
    
    
</head>
<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Action("Index","Login")">Bakery Manager</a>
            </div>
            <!-- /.navbar-header -->
            <ul class="nav navbar-top-links navbar-right" >
                @Html.Partial("_HeaderPartial")
            </ul>
                <!-- /.navbar-top-links -->

                @Html.Partial("_SidebarPartial")

</nav>

        <div id="page-wrapper" style="opacity: .95">
            @RenderBody()

        </div>



    </div>


    


   @RenderSection("scripts", false)


  @Html.Partial("StartupScript")

</body>
</html>